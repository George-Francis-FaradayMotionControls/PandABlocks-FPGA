# AUTOGENERATED

# Constraints for SFP sites?
{% if sfpsites %}
    {% for i in range(sfpsites) %}
read_xdc $TARGET_DIR/const/{{const[i]}}
    {% endfor %}
{% endif %}

# All IP code required
{% for ip in ips %}
read_ip $IP_DIR/{{ ip }}/{{ ip }}.xci
{% endfor %}

# Module code and constraints
{% for block in blocks %}
	{# Soft block module code #}
	{% if block.type in "dma|soft|sfp|fmc" %}
read_vhdl [glob $TOP_DIR/modules/{{ block.entity }}/hdl/*.vhd]
add_files $TOP_DIR/modules/{{ block.entity }}/hdl/
	{% endif %}
	{# Constraints #}
	{% if block.constraints %}
		{% for constraint in block.constraints %}
read_xdc $TOP_DIR/modules/{{ block.entity }}/{{ constraint }}
		{% endfor %}
	{% endif %}
	{% if block.otherconst %}
source $TOP_DIR/modules/{{ block.entity }}/{{ block.otherconst }}
    {% endif %}
{% endfor %}

