    ENABLE          bit_mux
    FRAME           bit_mux
    CAPTURE         bit_mux
    ERR_STATUS      read        enum 4
        0   Ok
        1   Capture before frame
        2   Multiple captures
        3   Buffer overrun
    ACTIVE          bit_out
    CAPTURE_TS      ext_out     timestamp
    FRAME_LENGTH    ext_out
    CAPTURE_OFFSET  ext_out     offset
    BITS0           ext_out     bits 0
    BITS1           ext_out     bits 1
