    WHEN_DISABLED   {{newBlockReg()}}
    SET_EDGE        {{newBlockReg()}}
    RST_EDGE        {{newBlockReg()}}
    FORCE_SET       {{newBlockReg()}}
    FORCE_RST       {{newBlockReg()}}
    ENABLE          {{newBlockReg(typ='bit_mux')}}
    SET             {{newBlockReg(typ='bit_mux')}}
    RST             {{newBlockReg(typ='bit_mux')}}
    OUT             {{newBitBus()}}


