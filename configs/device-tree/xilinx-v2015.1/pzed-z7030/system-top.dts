/dts-v1/;
/include/ "system.dts"

/ {
	model = "PZ_7z030_FMC_Carrier";
	aliases {
		ethernet0 = &gem0;
		serial0 = &uart1;
	};
	chosen {
		bootargs = "console=ttyPS0,115200 root=/dev/ram rw earlyprintk";
	};
	memory {
		device_type = "memory";
		reg = <0x0 0x40000000>;
	};
};

&gem0 {
	local-mac-address = [00 0a 35 00 22 5c];
};

&qspi {
	#address-cells = <1>;
	#size-cells = <0>;
	flash0: flash@0 {
		compatible = "micron,n25q128a13";
		reg = <0x0>;
		#address-cells = <1>;
		#size-cells = <1>;
		spi-max-frequency = <50000000>;
		partition@0x00000000 {
			label = "boot";
			reg = <0x0 0x500000>;
		};
		partition@0x00500000 {
			label = "bootenv";
			reg = <0x500000 0x20000>;
		};
		partition@0x00520000 {
			label = "data";
			reg = <0x520000 0xae0000>;
		};
	};
};

/ {
};

&gem0 {
	phy-handle = <&phy0>;
	phy-mode = "rgmii-id";
	
	mdio {
		#address-cells = <1>;
		#size-cells = <0>;
		phy0: phy@0 {
			compatible = "marvell,88e1512";
			device_type = "ethernet-phy";
			reg = <0x0>;
			marvell,reg-init = <3 16 0xff00 0x1e 3 17 0xfff0 0x00>;
		};
	};
};

/ {
	amba_pl: amba_pl {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "simple-bus";
		ranges ;
		panda_pcap_0: panda_pcap@43c10000 {
			compatible = "xlnx,panda-pcap-1.0";
			interrupt-parent = <&intc>;
			interrupts = <0 29 1>;
			reg = <0x43c10000 0x10000>;
			xlnx,s00-axi-addr-width = <0x7>;
			xlnx,s00-axi-data-width = <0x20>;
		};
	};
};


